<Window x:Class="InventoryManager.orderpicking"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InventoryManager"
        mc:Ignorable="d"
        Title="orderpicking" Height="500" Width="500">
    <Grid>
        <ListBox x:Name="orders_listBox"       HorizontalAlignment="Left" Height="280" Margin="40,37,0,0"  VerticalAlignment="Top" Width="130" SelectionChanged="orders_listBox_SelectionChanged"/>
        <ListBox x:Name="orderDetails_listBox" HorizontalAlignment="Left" Height="280" Margin="192,37,0,0" VerticalAlignment="Top" Width="260" SelectionChanged="orderDetails_listBox_SelectionChanged"/>

        <Button x:Name="complete_button" Content="Complete Order" HorizontalAlignment="Left" Margin="352,336,0,0" VerticalAlignment="Top" Width="100" Click="complete_button_Click" IsEnabled="False"/>


        <Button x:Name="button1" Content="Back" HorizontalAlignment="Left" Margin="377,413,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>


    </Grid>
</Window>
