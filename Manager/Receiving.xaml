<Window x:Class="InventoryManager.Receiving"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InventoryManager"
        mc:Ignorable="d"
        Title="Receiving" Height="500" Width="500">
    <Grid>
        <ListBox x:Name="orderDetails_listBox" HorizontalAlignment="Left" Height="319" Margin="231,10,0,0" VerticalAlignment="Top" Width="251"/>
        <ListBox x:Name="orders_listBox" HorizontalAlignment="Left" Height="319" Margin="13,10,0,0" VerticalAlignment="Top" Width="194" SelectionChanged="orders_listBox_SelectionChanged"/>
        <Button x:Name="completeOrder_button" Content="Complete Order" HorizontalAlignment="Left" Margin="231,368,0,0" VerticalAlignment="Top" Width="98" IsEnabled="False" Click="completeOrder_button_Click"/>
        <Button x:Name="button1" Content="Back" HorizontalAlignment="Left" Margin="407,439,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click" RenderTransformOrigin="0.28,-1.2"/>
        <Button x:Name="button2" Content="create order" HorizontalAlignment="Left" Margin="366,348,0,0" VerticalAlignment="Top" Width="116" Height="61" Click="button2_Click"/>


    </Grid>
</Window>
